{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "aelf-web3": ["types"]
    },
    "esModuleInterop": true,
    "declaration": true
  }
}
