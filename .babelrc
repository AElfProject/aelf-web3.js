{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "modules": false,
        "targets": "> 0.5%, last 2 versions, Firefox ESR, not dead"
      }
    ]
  ],
  "plugins": [
    "rewire"
  ],
  "ignore": [
    "dist/*.js"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 3,
            "modules": "auto"
          }
        ]
      ],
      "plugins": [
        "rewire"
      ],
      "ignore": [
        "dist/*.js"
      ]
    }
  }
}
